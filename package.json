{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "api created during the course Node Js, Typescript, TDD, DDD, Clean Architecture and Solid",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mefistofeles66/clean-node-api.git"
  },
  "keywords": [
    "NodeJs",
    "Typescript",
    "TDD",
    "DDD"
  ],
  "author": "Gabriel Costa Siqueira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mefistofeles66/clean-node-api/issues"
  },
  "homepage": "https://github.com/mefistofeles66/clean-node-api#readme",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "git-commit-msg-linter": "^3.1.0",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "validator": "^13.5.2"
  }
}
